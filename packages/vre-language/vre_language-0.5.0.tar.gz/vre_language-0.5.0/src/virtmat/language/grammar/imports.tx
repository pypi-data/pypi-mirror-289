/* object imports */

import identifiers

ObjectImports:
  ('use' namespace = TrueID '.' names = ObjectImport) |
  ('use' names += ObjectImport[','] ('from' namespace += TrueID['.'])?)
;

ObjectImport:
  name = TrueID (namespace += /\w$/)?
  // requires object processor to obtain 'namespace' from its parent
;
