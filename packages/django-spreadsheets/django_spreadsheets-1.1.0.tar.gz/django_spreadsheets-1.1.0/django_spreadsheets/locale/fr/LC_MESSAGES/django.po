msgid "{message}, cell {get_column_letter(self.column)}{self.row}"
msgstr "{message}, cellule {get_column_letter(self.column)}{self.row}"

#: errors.py:40
#, python-brace-format
msgid "The sheet \"{self.sheet_name}\" is missing"
msgstr "La feuille « {self.sheet_name} » est manquante"

#: errors.py:44
#, python-brace-format
msgid ""
"The sheet \"{self.sheet_name}\" is missing from the file you supplied. Check "
"that the sheet has not been deleted or renamed."
msgstr ""
"La feuille « {self.sheet_name} » est manquante dans le fichier fourni. "
"Veuillez vérifier que la feuille n’a pas été renommée ou supprimée."

#: errors.py:55
#, python-brace-format
msgid "The sheet \"{self.sheet_name}\" will be ignored when importing"
msgstr "La feuille « {self.sheet_name} » sera ignorée lors de l’import"

#: errors.py:59
msgid ""
"The sheet \"{self.sheet_name}\" in the file you have supplied does not "
"correspond to a sheet in the template ({\", \".join(self."
"config_sheets_names)}). It will be ignored."
msgstr ""
"La feuille « {self.sheet_name} » dans le fichier fourni ne correspond à "
"aucune feuille dans le modèle ({\", \".join(self.config_sheets_names)}). "
"Elle sera ignorée."

#: errors.py:72
msgid "The column name does not match the model"
msgstr "Le nom de la colonne ne correspond pas au modèle"

#: errors.py:77
#, python-brace-format
msgid ""
"{message} : the column name is \"{self.column_header}\" when it should be "
"\"{self.expected_column_header}\""
msgstr ""
"{message} : le nom de la colonne est « {self.column_header} » alors qu’il "
"devrait être « {self.expected_column_header} »"

#: errors.py:91
#, python-brace-format
msgid ""
"{message}, column \"{self.column_header}\", type is invalid. {self."
"error_message}"
msgstr ""
"{message}, colonne « {self.column_header} », le type est invalide. {self."
"error_message}"

#: errors.py:97
#, python-brace-format
msgid ""
"Column \"{self.column_header}\" must be of the type: {self.expected_type}"
msgstr ""
"La colonne « {self.column_header} » doit être de type : {self.expected_type}"

#: errors.py:108
#, python-brace-format
msgid "{message} : cell must no be empty"
msgstr "{message} : la cellule ne doit pas être vide"

#: errors.py:112
#, python-brace-format
msgid "Column \"{self.column_header}\" is required"
msgstr "La colonne « {self.column_header} » est requise"

#: errors.py:125
#, python-brace-format
msgid ""
"{message} at least one of the columns \"{self.column_header}\" or \"{self."
"unless_column_header}\" must be completed"
msgstr ""
"{message} au moins une des colonne « {self.column_header} » ou « {self."
"unless_column_header} » doit être remplie"

#: errors.py:131
#, python-brace-format
msgid ""
"At least one of the columns \"{self.column_header}\" or \"{self."
"unless_column_header}\" must be completed"
msgstr ""
"Au moins une des colonnes « {self.column_header} » ou « {self."
"unless_column_header} » doit être remplie"

#: errors.py:145
#, python-brace-format
msgid ""
"{message} column \"{self.column_header}\" must be completed if column "
"\"{self.required_if_column_header}\" is completed"
msgstr ""
"{message} la colonne « {self.column_header} » doit être remplie si la "
"colonne « {self.required_if_column_header} » est remplie"

#: errors.py:151
#, python-brace-format
msgid ""
"Columns \"{self.column_header}\" and \"{self.required_if_column_header}\" "
"must be completed"
msgstr ""
"Les colonnes « {self.column_header} » et « {self.required_if_column_header} "
"» doivent être remplies"

#: errors.py:163
#, python-brace-format
msgid "Line {self.line_number} is a duplicate of line {self.is_duplicate_of}"
msgstr ""
"La ligne {self.line_number} est un doublon de la ligne {self.is_duplicate_of}"

#: errors.py:168
msgid "Duplicated lines"
msgstr "Lignes dupliquées"

#: errors.py:179
#, python-brace-format
msgid "The object \"{self.object}\" will be deleted during import"
msgstr "L’objet « {self.object} » sera supprimé lors de l’import"

#: errors.py:184
#, python-brace-format
msgid ""
"The object \"{self.object}\" will be deleted during import because it does "
"not exist in the imported file."
msgstr ""
"L’objet « {self.object} » sera supprimé lors de l’import car il n’existe pas "
"dans le fichier fourni."

#: errors.py:192
msgid "unknown"
msgstr "inconnu"

#: errors.py:196
msgid "The configuration version of the file is incorrect"
msgstr "La version de configuration du fichier est incorrecte"

#: errors.py:200
#, python-brace-format
msgid ""
"The configuration version of the file ({self.found}) is different from that "
"expected ({self.expected})"
msgstr ""
"La version de configuration du fichier ({self.found}) est différente de "
"celui attendu ({self.expected})"

#: exceptions.py:11
#, python-brace-format
msgid "`{model_name}.{field_name}` received an unknown value: \"{value}\"."
msgstr "`{model_name}.{field_name}` a reçu une valeur inconnue: « {value} »."

