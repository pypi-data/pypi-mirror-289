include LICENSE
include README.md
include pyproject.toml
include jupyter-config/amazon_codewhisperer_jupyterlab_ext.json

recursive-include amazon_codewhisperer_jupyterlab_ext *

include package.json
include ts*.json

graft amazon_codewhisperer_jupyterlab_ext/labextension

# Javascript files
graft src
graft style
prune **/node_modules
prune lib

# Patterns to exclude from any directory
global-exclude *~
global-exclude *.pyc
global-exclude *.pyo
global-exclude .git
global-exclude .ipynb_checkpoints
