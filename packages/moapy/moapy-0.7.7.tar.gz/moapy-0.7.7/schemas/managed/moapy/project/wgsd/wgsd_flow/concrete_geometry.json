{
  "openapi": "3.1.0",
  "info": {
    "title": "moapy",
    "description": "Schema for moapy",
    "version": "0.1.0"
  },
  "servers": [
    {
      "url": "https://moa.rpm.kr-dv-midasit.com/backend/function-executor/python-execute/"
    },
    {
      "url": "https://moa.rpm.kr-st-midasit.com/backend/function-executor/python-execute/"
    }
  ],
  "paths": {
    "/moapy/project/wgsd/wgsd_flow/concrete_geometry": {
      "post": {
        "summary": "Concrete Geometry",
        "description": "Return the concrete geometry\n\nArgs:\n    Polygon (Points): concrete polygon\n    matl (ConcreteGrade): concrete material\n\nReturns:\n    ConcreteGeometry: concrete geometry",
        "operationId": "concrete geometry",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/concrete_geometry"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ConcreteGeometry"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ConcreteGeometry": {
        "properties": {
          "outerPolygon": {
            "items": {
              "$ref": "#/components/schemas/Point"
            },
            "type": "array",
            "title": "Outerpolygon",
            "description": "Outer polygon of the concrete",
            "default": [
              {
                "x": 0.0,
                "y": 0.0
              },
              {
                "x": 400.0,
                "y": 0.0
              },
              {
                "x": 400.0,
                "y": 600.0
              },
              {
                "x": 0.0,
                "y": 600.0
              }
            ]
          },
          "innerPolygon": {
            "items": {
              "$ref": "#/components/schemas/Point"
            },
            "type": "array",
            "title": "Innerpolygon",
            "description": "Inner polygon of the concrete",
            "default": []
          },
          "material": {
            "description": "Material of the concrete",
            "default": {
              "design_code": "ACI318M-19",
              "grade": "C12"
            },
            "$ref": "#/components/schemas/ConcreteGrade"
          }
        },
        "type": "object",
        "title": "GSD Concrete Geometry",
        "dataclassname": "moapy.project.wgsd.wgsd_flow.ConcreteGeometry"
      },
      "ConcreteGrade": {
        "properties": {
          "design_code": {
            "type": "string",
            "title": "Design Code",
            "description": "Design code",
            "default": "ACI318M-19"
          },
          "grade": {
            "type": "string",
            "title": "Grade",
            "description": "Grade of the concrete",
            "default": "C12"
          }
        },
        "type": "object",
        "title": "GSD Concrete Grade",
        "dataclassname": "moapy.project.wgsd.wgsd_flow.ConcreteGrade"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "Point": {
        "properties": {
          "x": {
            "type": "number",
            "title": "X"
          },
          "y": {
            "type": "number",
            "title": "Y"
          }
        },
        "type": "object",
        "required": [
          "x",
          "y"
        ],
        "title": "Point",
        "dataclassname": "moapy.project.wgsd.wgsd_flow.Point"
      },
      "Points": {
        "properties": {
          "points": {
            "items": {
              "$ref": "#/components/schemas/Point"
            },
            "type": "array",
            "title": "Points",
            "description": "Points",
            "default": [
              {
                "x": 0.0,
                "y": 0.0
              },
              {
                "x": 400.0,
                "y": 0.0
              },
              {
                "x": 400.0,
                "y": 600.0
              },
              {
                "x": 0.0,
                "y": 600.0
              }
            ]
          }
        },
        "type": "object",
        "title": "GSD Points",
        "dataclassname": "moapy.project.wgsd.wgsd_flow.Points"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      },
      "concrete_geometry": {
        "properties": {
          "Polygon": {
            "$ref": "#/components/schemas/Points"
          },
          "matl": {
            "$ref": "#/components/schemas/ConcreteGrade"
          }
        },
        "type": "object",
        "required": [
          "Polygon",
          "matl"
        ],
        "title": "concrete geometry"
      }
    }
  }
}