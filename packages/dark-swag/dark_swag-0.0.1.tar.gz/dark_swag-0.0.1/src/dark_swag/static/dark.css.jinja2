.expand-operation .arrow path {
    fill: rgba(255, 255, 255, 0.8);
}

.highlight-code pre {
    background-color: rgba(0, 0, 0, 1);
}

.models-control path {
    fill: rgba(255, 255, 255, 0.8);
}

.models-control span {
    color: rgba(255, 255, 255, 0.8);
    background-color: rgba(0, 0, 0, 0);
}

.opblock-summary-delete .arrow path {
    fill: rgba(249, 62, 62, 1);
}

.opblock-summary-get .arrow path {
    fill: rgba(97, 175, 254, 1);
}

.opblock-summary-patch .arrow path {
    fill: rgba(0, 255, 247, 1);
}

.opblock-summary-post .arrow path {
    fill: rgba(73, 204, 77, 1);
}

.opblock-summary-put .arrow path {
    fill: rgba(252, 161, 48, 1);
}

.swagger-ui .btn {
    color: rgba(255, 255, 255, 0.9);
    border: 2px solid rgba(3, 255, 36, 0.9);
}

.swagger-ui .btn.cancel {
    color: rgba(255, 255, 255, 0.9);
    border: 2px solid rgba(252, 33, 33, 0.9);
}

.swagger-ui .download-contents, .swagger-ui .copy-to-clipboard {
    border: 1px solid rgba(25, 255, 25, 0.5);
    background-color: rgba(0, 0, 0, 0.7);
    color: rgba(255, 255, 255, 0.8);
}

.swagger-ui .info .title {
    color: #ffffff;
}

.swagger-ui .info {
    margin: 6px 0 6px 0;
}

.swagger-ui .information-container {
    margin-top: 0;
    margin-bottom: 60px;
}

.swagger-ui .json-schema-2020-12 {
    background-color: rgba(50, 50, 50, 0.3);
}

.swagger-ui .json-schema-2020-12-keyword__name--primary {
    color: rgba(250, 134, 57, 0.9);
}

.swagger-ui .json-schema-2020-12-keyword__name--secondary {
    color: rgba(250, 134, 57, 0.8);
}

.swagger-ui .json-schema-2020-12-keyword__value--const {
    color: rgba(200, 117, 255, 0.8);
    border: 1px dashed rgba(200, 117, 255, 0.8);
}

.swagger-ui .json-schema-2020-12-property .json-schema-2020-12__title {
    color: rgba(153, 148, 255, 0.95);
}

.swagger-ui .json-schema-2020-12__attribute--primary {
    color: rgb(162, 252, 162);
}

.swagger-ui .json-schema-2020-12__title {
    color: rgba(255, 255, 255, 0.8);
}

.swagger-ui .model-box {
    background-color: rgba(58, 58, 58, 0.2);
    color: rgb(162, 252, 162);
}

.swagger-ui .models button {
    background-color: rgba(0, 0, 0, 0);
    color: rgba(255, 255, 255, 0.9);
}

.swagger-ui .models path {
    fill: rgba(255, 255, 255, 1);
}

.swagger-ui .no-margin {
    margin-left: 16px;
}

.swagger-ui .models .no-margin {
    margin-left: auto;
}

.swagger-ui .opblock .opblock-section-header h4 {
    color: rgba(255, 255, 255, 0.9);
}

.swagger-ui .opblock .opblock-section-header {
    background-color: rgba(56, 56, 56, 0.3);
}

.swagger-ui .opblock .opblock-summary-description {
    color: #cccccc;
}

.swagger-ui .opblock .opblock-summary-method {
    background-color: #0f6ab4;
}

.swagger-ui .opblock .opblock-summary-path {
    color: #ffffff;
}

.swagger-ui .opblock {
    background-color: #2d2d2d;
    border-color: #3a3a3a;
}

.swagger-ui .opblock-control-arrow {
    margin-left: 8px;
}

.swagger-ui .opblock-description-wrapper h4 {
    color: #ffffff;
}

.swagger-ui .opblock-description-wrapper p {
    color: #cccccc;
}

.swagger-ui .opblock-tag {
    color: #ffffff;
    background-color: rgba(0, 0, 0, 0);
    border-bottom: 1px solid rgba(255, 255, 255, 0.3);
    margin-bottom: 12px;
}

.swagger-ui .opblock.opblock-delete .view-line-link {
    border: 1px solid rgba(249, 62, 62, 0.5);
}

.swagger-ui .opblock.opblock-delete {
    background-color: rgba(249, 62, 62, 0.05);
}

.swagger-ui .opblock.opblock-get .view-line-link {
    border: 1px solid rgba(97, 175, 254, 0.5);
}

.swagger-ui .opblock.opblock-get {
    background-color: rgba(97, 175, 254, 0.05);
}

.swagger-ui .opblock.opblock-patch .view-line-link {
    border: 1px solid rgba(0, 255, 247, 0.5);
}

.swagger-ui .opblock.opblock-patch {
    background-color: rgba(0, 255, 247, 0.05);
}

.swagger-ui .opblock.opblock-post .view-line-link {
    border: 1px solid rgba(73, 204, 77, 0.5);
}

.swagger-ui .opblock.opblock-post {
    background-color: rgba(73, 204, 77, 0.05);
}

.swagger-ui .opblock.opblock-put .view-line-link {
    border: 1px solid rgba(252, 161, 48, 0.5);
}

.swagger-ui .opblock.opblock-put {
    background-color: rgba(252, 161, 48, 0.05);
}

.swagger-ui .parameter__name .required {
    color: rgba(255, 0, 0, 0.9);
}

.swagger-ui .parameter__name, .swagger-ui .parameter__type {
    color: #ffffff;
}

.swagger-ui .parameters-col_description input {
    background-color: rgba(0, 0, 0, 0.3);
    border: 1px solid rgba(255, 255, 255, 0.3);
    color: rgba(255, 255, 255, 0.9);
}

.swagger-ui .renderedMarkdown p {
    color: rgba(255, 255, 255, 0.8);
}

.swagger-ui .response-col_links {
    color: #ffffff;
}

.swagger-ui .response-col_status {
    color: #ffffff;
}

.swagger-ui .response-control-media-type--accept-controller select {
    color: #ffffff;
    border: 2px solid rgba(10, 183, 50, 1);
}

.swagger-ui .responses-inner h4, .swagger-ui .responses-inner h5 {
    color: #ffffff;
}

.swagger-ui .scheme-container {
    background-color: #2d2d2d;
}

.swagger-ui .tab li {
    color: #ffffff;
}

.swagger-ui .table-container {
    background-color: rgba(45, 45, 45, 0.3);
}

.swagger-ui .topbar {
    background-color: #2d2d2d;
}

.swagger-ui .view-line-link {
    border: 1px solid rgba(255, 255, 255, 0.5);
    fill: rgba(255, 255, 255, 0.3);
}

.swagger-ui a.nostyle {
    color: rgba(255, 255, 255, 0.8);
}

.swagger-ui input[type="text"] {
    background-color: rgba(0, 0, 0, 0.3);
    border: 1px solid rgba(255, 255, 255, 0.2);
}

.swagger-ui input[type="text"], .swagger-ui textarea {
    background-color: rgba(45, 45, 45, 0.3);
    color: #ffffff;
}

.swagger-ui section.models {
    border: 1px solid rgba(255, 0, 166, 0.9);
    background-color: rgba(125, 18, 40, 0.05);
}

.swagger-ui select {
    background-color: rgba(0, 0, 0, 1);
    border: 1px solid rgba(255, 255, 255, 0.6);
    color: rgba(255, 255, 255, 0.9);
    background-image: url("data:image/svg+xml;utf8,<svg fill='white' height='24' viewBox='0 0 24 24' width='24' xmlns='http://www.w3.org/2000/svg'><path d='M7 10l5 5 5-5z'/></svg>");
    background-repeat: no-repeat;
    background-position-x: 100%;
    background-position-y: 50%;
}

.swagger-ui table thead tr td, .swagger-ui table thead tr th {
    border-bottom-color: rgba(225, 225, 225, 0.5) !important;
    color: #ffffff;
}

.swagger-ui {
    min-width: 510px;
    color: #ffffff;
    background-color: rgba(55, 55, 55, 0.1);
    backdrop-filter: blur(6px);
    padding-top: 8px;
    border: 1px solid rgba(10, 133, 199, 0.6) !important;
    border-radius: 10px;
    margin: 40px auto;
    max-width: 1460px;
    {{ logo }}
}

body {
    background-color: rgba(0, 0, 0, 1);
    color: #ffffff;
    margin: 0 40px;
    padding: 0;
    min-height: 100vh;
    {{ background_text }}
}